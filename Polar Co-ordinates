// Cartesian to polar co-ordinates conversion
#include <stdio.h>
#include<math.h>
int main()
{
    float r,x,y,phi;    
   printf("The cartesian co-ordinates of a point are x= ");
   scanf ("%f", &x);
   printf(" y= ");
   scanf ("%f", &y);
   r = sqrt(x*x+y*y);
   //phi = 1/ tan(y/x);
   phi = atan2(y,x);
   phi = phi * 180/3.14;
  
   printf("The polar co-ordinates are r=%f, phi=%f ", r,phi);
   
   
}
